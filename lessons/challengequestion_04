"""Dict Function Writing."""

__author__ = "730622763"

def zip(first_list: list[str], second_list: list[int]) -> dict[str, int]:
    if len(first_list) != len(second_list):
        return {}
    result: dict[str, int] = {}
    for key in first_list:
        result[key] = first_list[key]
    return result

